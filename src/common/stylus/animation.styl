@keyframes jump
  0%
    transform translateY(0)
  75%
    transform translateY(-50%)
  100%
    transform translateY(0)

@keyframes diving
  0%
    transform translateY(0)
  75%
    transform translateY(50%)
  100%
    transform translateY(0)
    
@keyframes filter-blur
  0%
    filter blur(0px)
  50%
    filter blur(5px)
  100%
    filter blur(0px)